<?xml version="1.0" encoding="UTF-8" ?>
<response>
	<command>{{ command }}</command>
	{% if direction is defined %}
		<direction>{{ direction }}</direction>
	{% endif %}
	{% if speed is defined %}
		<speed>{{ speed}}</speed>
	{% endif %}
	{% if result is defined %}
		<result>{{ result }}</result>
	{% endif %}
</response>